generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = "mysql://ark25_onlysimply:2cf8076858aef2e1c27a6a762ea2c63eba77f763@lgcl5.h.filess.io:3306/ark25_onlysimply"
}

model boats {
  id          String @id
  name        String
  description String @db.VarChar(1000)
  image       String
  source      String
}

model litographyartwork {
  id                 String           @id
  title              String
  image              String
  category           String
  description        String           @db.VarChar(1000)
  litographyAuthorId String
  litographyauthor   litographyauthor @relation(fields: [litographyAuthorId], references: [id], map: "LitographyArtwork_litographyAuthorId_fkey")

  @@index([litographyAuthorId], map: "LitographyArtwork_litographyAuthorId_fkey")
}

model litographyauthor {
  id                String              @id
  author            String
  nickname          String
  year              String
  proffession       String
  picture           String
  story             String              @db.VarChar(1000)
  nationality       String?
  litographyartwork litographyartwork[]
}

model prehistoric {
  name        String
  id          Int    @id @default(autoincrement())
  age         String
  button      String
  assets      String
  description String @db.VarChar(1000)
}
